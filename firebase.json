{
  "hosting": {
    "public": "build",
    "redirects": [
      {
        "source": "/public/fonts/:font*",
        "destination": "/fonts/:font*",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source" : "/fonts/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=604800"
      } ]
    }]
  }
}
